{
    "name": "model",
    "version": "0.0.2",
    "description": "Data and Rules model for WowFinder",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/WowFinder/model.git",
    "author": "Edurne Pascual <herenvardo@gmail.com>",
    "license": "GPL3",
    "private": true,
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "eslint . --ext js,ts",
        "lint:fix": "eslint . --ext js,ts --fix",
        "prettier": "prettier --write \"src/**/*.{js,ts,json,json5}\"",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "tsc": "tsc",
        "checks:quick": "npm-run-all -pc lint tsc",
        "checks:standard": "npm-run-all -pc lint tsc test",
        "checks:pedantic": "npm-run-all -sc lint:fix prettier build test:coverage",
        "build:pre": "rimraf dist",
        "build:tsc": "tsc",
        "build": "npm-run-all -s build:pre build:tsc",
        "prepublishOnly": "yarn run build",
        "prepare": "yarn run build"
    },
    "dependencies": {
        "@wowfinder/asset-schemas": "0.0.7",
        "@wowfinder/ts-enums": "^0.0.5",
        "@wowfinder/ts-utils": "^0.0.8"
    },
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.3",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.7.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.5",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.2",
        "tsd": "^0.30.7",
        "typescript": "^5.4.2"
    },
    "packageManager": "yarn@4.1.1"
}
